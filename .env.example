# OrbGuard Lab - Environment Variables Template
# Copy this file to .env and fill in your API keys
# DO NOT commit .env to git

# ============================================
# Premium Threat Intelligence API Keys
# ============================================

# Koodous - Android Malware Analysis
# Get your API key at: https://koodous.com/settings/api-integration
ORBGUARD_SOURCES_KOODOUS_API_KEY=your_koodous_api_key

# AlienVault OTX - Open Threat Exchange
# Get your API key at: https://otx.alienvault.com/settings
ORBGUARD_SOURCES_ALIENVAULT_OTX_API_KEY=your_alienvault_otx_api_key

# GreyNoise - IP Intelligence
# Get your API key at: https://viz.greynoise.io/account/api-key
ORBGUARD_SOURCES_GREYNOISE_API_KEY=your_greynoise_api_key

# AbuseIPDB - IP Reputation
# Get your API key at: https://www.abuseipdb.com/account
ORBGUARD_SOURCES_ABUSEIPDB_API_KEY=your_abuseipdb_api_key

# VirusTotal - Comprehensive Threat Intelligence
# Get your API key at: https://www.virustotal.com/gui/user/<username>/apikey
ORBGUARD_SOURCES_VIRUSTOTAL_API_KEY=your_virustotal_api_key

# Google Safe Browsing - Phishing/Malware URL Detection (PhishTank alternative)
# Get your API key at: https://console.cloud.google.com/apis/credentials
# Enable the Safe Browsing API in your Google Cloud project
ORBGUARD_SOURCES_GOOGLE_SAFEBROWSING_API_KEY=your_google_safebrowsing_api_key

# ============================================
# Database & Infrastructure
# ============================================
ORBGUARD_DATABASE_PASSWORD=orbguard_secret
ORBGUARD_REDIS_PASSWORD=
ORBGUARD_NEO4J_PASSWORD=neo4j_secret
ORBGUARD_JWT_SECRET=your-secure-jwt-secret-change-in-production
